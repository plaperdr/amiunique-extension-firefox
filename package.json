{
  "title": "AmIUnique",
  "name": "amiunique-extension",
  "version": "1.0.1",
  "description": "An extension to follow the evolution of your browser fingerprint! Be notified as soon as a change is detected!",
  "main": "index.js",
  "author": "Pierre Laperdrix",
  "homepage": "https://amiunique.org",
  "engines": {
    "firefox": ">=38.0a1",
    "fennec": ">=38.0a1"
  },
  "license": "GPLv3",
  "permissions": {
    "cross-domain-content": ["https://amiunique.org/"]
  },
  "preferences":[{
    "name": "notifications",
    "type": "radio",
    "title": "Notifications",
    "value": "E",
    "options": [
      {
        "value": "E",
        "label": "Enabled"
      },
      {
        "value": "D",
        "label": "Disabled"
      },
      {
        "value": "B",
        "label": "Disable badge only"
      }
    ]
  },{
    "type": "control",
    "label": "Show ID",
    "name": "showID",
    "title": "AmIUnique ID"
  }]

}
